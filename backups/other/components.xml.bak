    $(&#039;.temathic-carousel .carousel-item&#039;).each(function(){
        var minPerSlide = 3;
        var next = $(this).next();
        if (!next.length) {
        next = $(this).siblings(&#039;:first&#039;);
        }
        next.children(&#039;:first-child&#039;).clone().appendTo($(this));
        
        for (var i=0;i&lt;minPerSlide;i++) {
            next=next.next();
            if (!next.length) {
                next = $(this).siblings(&#039;:first&#039;);
            }
            
            next.children(&#039;:first-child&#039;).clone().appendTo($(this));
          }
    });

